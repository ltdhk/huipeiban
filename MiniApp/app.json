{
  "darkmode": true,
  "pages": [
    "pages/home/home",
    "pages/ai-chat/ai-chat",
    "pages/companions/list",
    "pages/companions/detail",
    "pages/institutions/list",
    "pages/institutions/detail",
    "pages/orders/list",
    "pages/orders/detail",
    "pages/messages/list",
    "pages/messages/chat",
    "pages/profile/index"
  ],
  "themeLocation": "theme.json",
  "usingComponents": {},
  "window": {
    "navigationBarTitleText": "会照护",
    "navigationStyle": "custom",
    "navigationBarTextStyle": "@navTxtStyle"
  },
  "tabBar": {
    "color": "@tabBarColor",
    "selectedColor": "@tabBarSelectedColor",
    "backgroundColor": "@tabBarBgColor",
    "borderStyle": "@tabBarBorderStyle",
    "list": [
      {
        "pagePath": "pages/home/home",
        "text": "首页",
        "iconPath": "assets/icons/home.png",
        "selectedIconPath": "assets/icons/home-active.png"
      },
      {
        "pagePath": "pages/ai-chat/ai-chat",
        "text": "AI陪诊",
        "iconPath": "assets/icons/ai.png",
        "selectedIconPath": "assets/icons/ai-active.png"
      },
      {
        "pagePath": "pages/orders/list",
        "text": "订单",
        "iconPath": "assets/icons/orders.png",
        "selectedIconPath": "assets/icons/orders-active.png"
      },
      {
        "pagePath": "pages/messages/list",
        "text": "消息",
        "iconPath": "assets/icons/messages.png",
        "selectedIconPath": "assets/icons/messages-active.png"
      },
      {
        "pagePath": "pages/profile/index",
        "text": "我的",
        "iconPath": "assets/icons/profile.png",
        "selectedIconPath": "assets/icons/profile-active.png"
      }
    ]
  },
  "sitemapLocation": "sitemap.json",
  "lazyCodeLoading": "requiredComponents",
  "rendererOptions": {
    "skyline": {
      "disableABTest": true,
      "defaultDisplayBlock": true,
      "defaultContentBox": true,
      "sdkVersionBegin": "3.4.3",
      "sdkVersionEnd": "15.255.255"
    }
  }
}
